<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:gravity="center_horizontal"
        android:layout_gravity="center_horizontal"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="match_parent"
            android:textSize="100px"
            android:text="Results" />
        <TextView
            android:id="@+id/name"
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="match_parent"
            android:textSize="50px"
            android:text="initials: " />
        <TextView
            android:id="@+id/group"
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="match_parent"
            android:textSize="50px"
            android:text="group: " />

        <TableLayout
            android:layout_width="370dp"
            android:layout_height="368dp"
            android:layout_marginTop="100dp"
            android:background="#d9d9d9"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal"
                >


                <TextView
                    android:id="@+id/textTime"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="Text Time: "
                    android:textSize="30px"
                    android:paddingLeft="25dp"
                    android:paddingRight="10dp"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/speechTime"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="Speech Time: "
                    android:textSize="30px"
                    android:paddingLeft="25dp"
                    android:paddingRight="10dp"/>
            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"

                android:orientation="horizontal">


                <TextView
                    android:id="@+id/textE"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Text Error rate: "
                    android:textSize="30px"
                    android:paddingLeft="25dp"
                    android:paddingRight="10dp"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/speechE"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Speech Error rate: "
                    android:textSize="30px"
                    android:paddingLeft="25dp"
                    android:paddingRight="10dp"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/complete"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Speech Tast Completed: "
                    android:textSize="30px"
                    android:paddingLeft="25dp"
                    android:paddingRight="10dp"/>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/complete2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Text Tast Completed: "
                    android:textSize="30px"
                    android:paddingLeft="25dp"
                    android:paddingRight="10dp"/>

            </TableRow>

            <TableRow android:layout_marginTop="100dp">

                <Button
                    android:id="@+id/Button01"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:layout_weight="1"
                    android:background="#c4eaff"
                    android:onClick="clickOK"
                    android:text="Back to Settings"
                    android:textAppearance="?android:attr/textAppearanceMedium"></Button>
            </TableRow>


        </TableLayout>


    </LinearLayout>
</LinearLayout>